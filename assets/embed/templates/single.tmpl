{{ define "content" }}
{{ with .Data}}
 <main class="container">
  {{ if eq .Type "image" }}
   <img src="{{ .URL }}?raw=true">
   {{ else if eq .Type "audio" }}
   <audio src="{{ .URL }}?raw=true"></audio>
   {{ else if eq .Type "video" }}
   <!-- TODO: SHOW VIDEO ? -->
   {{ else if eq .Type "blob" }}
   <a href="?download=true">Download</a>
   {{ else}}
   <pre>{{ .StringifyContent }}</pre>
  {{ end }}
 </main>
{{ end }}
{{ end }}
